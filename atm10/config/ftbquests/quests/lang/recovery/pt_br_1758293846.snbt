	"chapters/ars_nouveau.snbt": {
		quest.5C3FF43CF16BCF30.quest_desc: {
			old: ["Com as Gemas de Fonte, você pode começar a criar as várias máquinas, criando &5Sourcestones&r."]
			new: ["Com as Gemas de Source, você pode começar a criar as várias máquinas, criando &5Pedras de Source&r."]
		}
		quest.77145113CD5B26FB.quest_subtitle: {
			old: "Gera Fonte de Comida Próxima"
			new: "Gera Source de Comida Próxima"
		}
		quest.6DAA82B5F94AF9F8.quest_desc: {
			old: ["A &9Jar of Light&r invoca uma fonte de luz flutuante que o segue. \\n\\nA &6Jarra do Vazio&r destrói os itens que você pega em troca de mana. Isso pode ser filtrado. \\n\\nPara adicionar ou remover um item a ser destruído pela jarra, use a jarra com o item em sua mão secundária, ou use um item na Mesa do Escriba com a jarra colocada sobre ela. \\n\\nA jarra deve estar na sua Barra de acesso rápido para funcionar."]
			new: ["O &9Vaso de Luz&r invoca uma fonte de luz flutuante que segue você. \\n\\nO &6Vaso do Vazio&r destrói itens que você coleta em troca de mana. Isso pode ser filtrado. \\n\\nPara adicionar ou remover um item a ser destruído pelo vaso, use o vaso com o item em sua mão secundária, ou use um item na Mesa do Escriba com o vaso colocado sobre ela. \\n\\nO vaso deve estar na sua barra de acesso rápido para funcionar."]
		}
		task.2BC0F5A88E2F03C3.title: {
			old: "Troncos de Archwood"
			new: "Archwood"
		}
		quest.457DE8C154641437.quest_desc: {
			old: ["O &9Ritual Brazier&r é usado para lançar rituais. Também pode ser usado para decoração. Se você quiser acender o Braseiro, use um feitiço de Luz nele. \\n\\nPara realizar rituais, você também precisará de Pedestais Arcanos. Como há tantos rituais para lançar, certifique-se de verificar cada um deles usando o &6Caderno Gasto&r! \\n\\nIsso pode ser feito segurando Ctrl na Mesa Ritual com o livro em seu inventário."]
			new: ["O &9Braseiro Ritualístico&r é usado para realizar rituais. Também pode ser usado como decoração. Se você quiser acender o Braseiro, use um feitiço de Luz nele. \\n\\nPara realizar rituais, você também precisará de Pedestais Arcanos. Como há muitos rituais para realizar, certifique-se de conferir cada um deles usando o &6Caderno Gasto&r! \\n\\nIsso pode ser feito segurando Ctrl na Mesa Ritualística enquanto o livro está em seu inventário."]
		}
		quest.19D02325579F2AA8.quest_desc: {
			old: ["O Elo de Fonte Vulcânico gera Fonte consumindo itens combustíveis. Troncos de Arcomadeira gerarão Fonte bônus. \\n\\nÀ medida que queima itens, ele converterá pedra próxima em lava. \\n\\nIsso também gera Lírios de Lava."]
			new: ["O Vínculo de Source Vulcânico gera Source consumindo itens combustíveis. Troncos de Archwood gerarão Source bônus. \\n\\nÀ medida que queima itens, ele converterá pedra próxima em lava. \\n\\nIsso também gera Lírios de Lava."]
		}
		quest.088FCB5267CB7A89.quest_desc: {
			old: ["Este cinto dará efeitos de poção positivos aleatórios por uma curta duração. Estes variarão em força."]
			new: ["Este cinto concederá efeitos de poção positivos aleatórios por um curto período. A força desses efeitos irá variar."]
		}
		quest.5766C8B9E850C186.quest_subtitle: {
			old: "Criando Gemas de Fonte"
			new: "Criando Gemas de Source"
		}
		quest.6B511C8B572E8940.quest_desc: {
			old: ["No canto inferior esquerdo da sua tela, você verá uma barra. Essa barra é sua reserva de mana! \\n\\nExistem várias maneiras de aumentar sua reserva de mana ou a eficiência de seus feitiços à medida que você progride no mod. Aprimorar seu Livro de Feitiços também pode aumentar sua mana!"]
			new: ["No canto inferior esquerdo da sua tela, você verá uma barra. Esta barra é sua reserva de mana! \\n\\nExistem várias maneiras de aumentar sua reserva de mana ou aumentar a eficiência de seus feitiços à medida que avança no mod. Aprimorar seu livro de feitiços também pode aumentar sua mana!"]
		}
		quest.542C6D76B579886C.quest_desc: {
			old: ["Usando nossa estrutura de Aparato de Encantamento, vamos querer criar nossa primeira semente, a &5Semente de Magebloom&r. \\n\\nIsso será usado para criar algumas roupas mágicas para nós!"]
			new: ["Usando essas sementes, vamos querer criar nossa primeira fibra, a &5Fibra de Magebloom&r. \\n\\nIsso será usado para criar alguns acessórios mágicos para nós!"]
		}
		quest.40BC67BDEE15D1DE.quest_desc: {
			old: ["Para criar Gemas da Fonte, precisaremos colocar Fragmentos de Lápis-lazúli ou Ametista na Imbuement Chamber. Com o tempo, eles se converterão em Gemas da Fonte!"]
			new: ["Para criar Gemas de Source, precisaremos colocar Lápis-lazúli ou Fragmentos de Ametista na Câmara de Imbuimento. Com o tempo, eles se converterão em Gemas de Source!"]
		}
		quest.5766C8B9E850C186.quest_desc: {
			old: [
				"Para criar um dos principais itens de criação em Ars, a &9Source Gem&r, precisaremos fazer uma &6Imbuement Chamber&r. \\n\\nA Imbuement Chamber requer Fonte para imbuir itens. Ela produz uma pequena quantidade de Source por si só, mas também pode usar Source Jars como fonte de energia. \\n\\nVárias receitas também exigem que Pedestais Arcanos sejam colocados ao redor dela.\\n"
				"{image:atm:textures/questpics/ars/imbuement.png width:200 height:150 align:1}"
			]
			new: [
				"Para criar um dos principais itens de fabricação no Ars, a &9Gema de Source&r, precisaremos fazer uma &6Câmara de Imbuimento&r. \\n\\nA Câmara de Imbuimento requer Source para imbuir itens. Ela produz uma pequena quantidade de Source por si só, mas também pode usar Vasos de Source como fonte de energia. \\n\\nDiversas receitas também requerem que Pedestais Arcanos sejam colocados ao redor dela.\\n"
				"{image:atm:textures/questpics/ars/imbuement.png width:200 height:150 align:1}"
			]
		}
		quest.3D862A3D3F83CA26.quest_desc: {
			old: [
				"O &9Aparelho de Encantamento&r é usado para criar vários itens dentro do mod. Ele requer Fonte para funcionar.\\n\\nÉ também uma estrutura multibloco que utiliza Pedestais Arcanos para a criação.\\n\\nComece colocando um Núcleo Arcano no chão, depois coloque o Aparelho de Encantamento em cima dele. Cerque o Aparelho com Pedestais Arcanos.\\n"
				"{image:atm:textures/questpics/ars/enchanting_app.png width:200 height:175 align:1}"
			]
			new: [
				"O &9Aparato de Encantamento&r é usado para criar vários itens dentro do mod. Ele requer Source para funcionar.\\n\\nTambém é uma estrutura multiblock que utiliza Pedestais Arcanos para a criação.\\n\\nComece colocando um Núcleo Arcano no chão, depois coloque o Aparato de Encantamento em cima dele. Cerque o Aparato com Pedestais Arcanos.\\n"
				"{image:atm:textures/questpics/ars/enchanting_app.png width:200 height:175 align:1}"
			]
		}
		quest.51162B9185A45BB1.quest_desc: {
			old: ["Este arco pode ser inscrito com um feitiço usando uma Mesa de Escriba. \\n\\nAo custo de mana, as flechas se tornarão Flechas de Feitiço e aplicarão o feitiço em seu alvo. \\n\\nSe você não tiver flechas, uma flecha de feitiço que causa 0 de dano será lançada. Se não houver mana suficiente, flechas normais serão disparadas. \\n\\nO &9Arco do Encantador&r também pode usar Flechas de Aumento especiais que fortalecem o feitiço inscrito."]
			new: ["Este arco pode ser inscrito com um feitiço usando uma Mesa do Escriba. \\n\\nAo custo de mana, as flechas se tornarão Flechas de Feitiço e aplicarão o feitiço em seu alvo. \\n\\nSe você não tiver flechas, uma flecha de feitiço que causa 0 de dano será lançada. Se não houver mana suficiente, flechas normais serão disparadas. \\n\\nO &9Arco do Encantador&r também pode usar Flechas de Aumento especiais que fortalecem o feitiço inscrito."]
		}
		quest.3D862A3D3F83CA26.title: {
			old: "Aparelho de Encantamento"
			new: "&9Aparato de Encantamento"
		}
		quest.0D330FAD6C993DBC.quest_desc: {
			old: ["Esta é a próxima melhoria para o seu Livro de Feitiços!\\n\\nIsso aumentará sua mana geral e regeneração de mana, além de permitir que você crie e use Glifos de Nível 2."]
			new: ["Esta é a próxima melhoria para o seu Livro de Feitiços! \\n\\nIsso aumentará sua mana total e regeneração de mana, além de permitir que você crie e use Glifos de Nível 2."]
		}
		quest.36CDA39C23D3AA2B.quest_desc: {
			old: ["Esta Missão foi criada pela &6Equipe AllTheMods&r, ou por um &2contribuidor da Comunidade&r para uso nos Modpacks AllTheMods. \\n\\nComo todos os pacotes &6AllTheMods&r são licenciados sob &eTodos os Direitos Reservados&r, esta Missão não pode ser usada em nenhum pacote público não lançado pela &6Equipe AllTheMods&r sem permissão explícita. \\n\\nEsta missão está intencionalmente oculta, se você está vendo isso, está no modo de edição."]
			new: ["Esta Missão foi criada pela &6Equipe AllTheMods&r, ou por um &2contribuidor da Comunidade&r para uso nos Modpacks AllTheMods. \\n\\nComo todos os pacotes &6AllTheMods&r são licenciados sob &eTodos os Direitos Reservados&r, esta Missão não pode ser usada em nenhum pacote público não lançado pela &6Equipe AllTheMods&r sem permissão explícita. \\n\\nEsta missão está intencionalmente oculta, se você está vendo isso, você está no modo de edição."]
		}
		quest.111649D7E16D869F.quest_desc: {
			old: ["A &9Varinha do Conjurador&r aceita apenas um único feitiço e é inscrita usando a Mesa do Escriba.\\n\\nOs feitiços da varinha sempre começam com Projétil > Acelerar, e DEVEM ser inscritos com um feitiço que não tenha outro método (como toque, si mesmo, etc).\\n\\nIsso permite que você lance feitiços além do limite de 10 feitiços. Se você quiser usar Quebrar, basta inscrever a varinha com Quebrar por si só."]
			new: ["A &9Varinha de Encantador&r aceita apenas um único feitiço e é inscrita usando a Mesa do Escriba. \\n\\nOs feitiços da varinha sempre começam com Projétil > Acelerar, e DEVEM ser inscritos com um feitiço que não tenha outro método (como toque, em si, etc.). \\n\\nIsso permite que você lance feitiços além do limite de 10 feitiços. Se você quiser usar Quebrar, basta inscrever a varinha apenas com Quebrar."]
		}
